      "source": [
        "db_filename = r'studentdata.db'\n",
        "con = lite.connect(db_filename)\n",
        "df.to_sql('student',\n",
        "con,\n",
        "schema=None,\n",
        "if_exists='replace',\n",
        "index=True,\n",
        "index_label=None,\n",
        "chunksize=None,\n",
        "dtype=None)\n",
        "con.close()\n",
        "\n",
        "db_file = r'studentdata.db'\n",
        "engine = create_engine(r\"sqlite:///{}\" .format(db_file))\n",
        "sql = 'SELECT * from student '\n",
        "\n",
        "\n",
        "studf = pd.read_sql(sql, engine)\n",
        "studf"
      ],
